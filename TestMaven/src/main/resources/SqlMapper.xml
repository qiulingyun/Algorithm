<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MySqlMapper">
  <select id="selectAllCity" resultType="Model.City">
    select * from world.city limit 100
  </select>
  <select id="selectCity" resultType="Model.City">
    select * from world.city where name = #{name}
  </select>
</mapper>